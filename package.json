    "@types/bun": "^1.1.8",
    "@types/node": "^22.5.4",
    "@typescript-eslint/parser": "^8.4.0",
    "dotenv": "^16.4.5",
    "eslint": "^9.10.0",
    "release-it": "^18.1.1",
